<div class="row">
	<% for day in workshop.schedule %>

	<table>
		<thead>
			<tr>
				<th colspan=2><%= day.day %></th>
			</tr>
		</thead>
		<tbody> <!-- Intro to HTML, CSS and Javascript -->
			<% for session in day.sessions %>
			<tr>
				<td class="schedule-label"><%= session.time %></td>
				<td class="schedule-description"><%= session.title %></td>
			</tr>
			<% end %>
		</tbody>
	</table>

	<% end %>
</div>